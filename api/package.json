    "mysql2": "^3.16.0",
    "swagger-ui-express": "^5.0.1",
    "tsx": "^4.21.0",
    "winston": "^3.11.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.4",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.17",
